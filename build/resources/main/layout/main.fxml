            xmlns:fx="http://javafx.com/fxml/1" fx:controller="utifile.controller.MainController">
    <children>
        <Button fx:id="clientButton" layoutX="14.0" layoutY="47.0" mnemonicParsing="false" text="Client"
                AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="60.0"/>
        <Button fx:id="serverButton" layoutX="14.0" layoutY="107.0" mnemonicParsing="false" text="Server"
