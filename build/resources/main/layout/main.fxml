                <Font size="16.0"/>
            </font>
        </Label>
        <Separator layoutX="14.0" layoutY="72.0" prefWidth="200.0" AnchorPane.leftAnchor="5.0"
                   AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="120.0"/>
        <Label layoutX="14.0" layoutY="150.0" text="Port:" AnchorPane.bottomAnchor="155.0" AnchorPane.leftAnchor="5.0"/>
        <Label layoutX="14.0" layoutY="205.0" text="Password:" AnchorPane.bottomAnchor="100.0"
               AnchorPane.leftAnchor="5.0"/>

        <TextField fx:id="serverPortField" layoutX="14.0" layoutY="166.0" AnchorPane.bottomAnchor="125.0"
                   AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="150.0"/>
        <PasswordField fx:id="serverPasswordField" layoutX="14.0" layoutY="221.0" AnchorPane.bottomAnchor="70.0"
                       AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="150.0"/>
    </children>
</AnchorPane>
